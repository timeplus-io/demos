          "refId": "A",
          "sql": "select _tp_time,instance,metrics_value from(\n  select _tp_time, array_join(raw:resourceMetrics[*]) as rm\n,(array_filter(x -> x:key = 'host.name' or x:key = 'instance', rm:resource.attributes[*])[1]):value.stringValue as instance,rm:scopeMetrics[*][1] as sm,\nreplace_regex(sm:scope.name,'.*scraper/(.*)','\\\\1') as scope_name\n,array_join(sm:metrics[*]) as metrics,metrics:description as metrics_desc\n,(metrics:gauge.dataPoints[*][1]):asDouble::double as metrics_value\nfrom o11y.otlp_metrics where _tp_time < to_datetime($__to/1000) and _tp_time > to_datetime($__from/1000) and scope_name='loadscraper' and metrics_desc='Average CPU Load over 1 minute.' \n);"
        }
      ],
      "title": "CPU",
      "transformations": [
        {
          "id": "prepareTimeSeries",
          "options": {
            "format": "multi"
          }
        },
        {
          "id": "renameByRegex",
          "options": {
            "regex": "metrics_value (.*)",
            "renamePattern": "$1"
          }
        }
      ],
      "type": "timeseries"
